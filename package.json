{
  "name": "prismagram",
  "version": "1.0.0",
  "description": "express + prisma + react + react native 인스타그램 클론 ",
  "main": "index.js",
  "repository": "https://github.com/aispark/prismagram.git",
  "author": "aispark <aispark@paran.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "babel-node": "^0.0.1-security",
    "copy": "^0.3.2",
    "dotenv": "^8.1.0",
    "graphql-tools": "^4.0.5",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "prisma-client-lib": "^1.34.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "nodemon": "^1.19.3"
  },
  "scripts": {
    "deploy": "prisma deploy",
    "generate": "prisma generate",
    "prisma": "yarn run deploy && yarn run generate",
    "dev": "nodemon --exec babel-node src/server.js",
    "prebuild": "yarn run prisma",
    "build": "babel src -d build",
    "postbuild": "cd src && npx copy api/**/*.graphql ../build/api",
    "start": "node build/server.js"
  }
}
